{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch edge",
            "type": "msedge",
            "request": "launch",
            "url": "http://localhost:4200/#",
            "webRoot": "${workspaceFolder}/front/web",
        },
        {
            "name": "Launch Backend",
            "program": "${workspaceFolder}/back/server.js",
            "envFile": "${workspaceFolder}/back/.env",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        }
    ]
}